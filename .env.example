# MarketingTool App Environment Variables
# Copy this file to .env and fill in your values

# Appwrite Configuration (already in app.json)
EXPO_PUBLIC_APPWRITE_ENDPOINT=https://api.marketingtool.pro/v1
EXPO_PUBLIC_APPWRITE_PROJECT_ID=6952c8a0002d3365625d

# AI API Keys (choose one or configure backend to use)
# Option 1: OpenAI API Key
EXPO_PUBLIC_OPENAI_API_KEY=sk-your-openai-api-key-here

# Option 2: Anthropic Claude API Key
EXPO_PUBLIC_ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Backend API (if using custom backend for AI)
EXPO_PUBLIC_AI_API_URL=https://api.marketingtool.pro/api/generate
